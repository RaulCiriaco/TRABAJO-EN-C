#include <stdio.h>

int main() {
    int arr[] = {1, 2, 3, 4, 5};  // Declaraci√≥n de un arreglo de enteros
    int *p = arr;  // Un puntero que apunta al primer elemento del arreglo

    for(int i = 0; i < 5; i++) {
        printf("arr[%d] = %d usando punteros: %d\n", i, arr[i], *(p + i));
    }

    return 0;
}
